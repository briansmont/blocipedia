<h1>All Wikis Available:</h1>


<div class="row">
  <div class="col-md-8">
    <% @wikis.each do |wiki| %>
      <div class="media">
        <h5 class="media-body">
          <%=link_to wiki.title, wiki %>
        </h5>
        <small>
          <%= wiki.body %>
        </small>
      </div>
    <% end %>
  </div>
</div>