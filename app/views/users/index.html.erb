<h1>Users</h1>

<div class="row">
  <div class="col-md-8">
    <% @users.each do |user| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            Name: <%= link_to user.email %> More information needed still, once user's have .name
          </h4>
        </div>
      </div>
    <% end %>
  </div>
</div>
